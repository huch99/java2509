package ch02.sec01;

public class VariableInitializationExample {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int value = 10;
		
		int hour = 3;
		int minute = 5;
		
		int result = value + 10;
		
		System.out.println(result);
		System.out.println(hour + '시간' + minute + "분");
	}

}
